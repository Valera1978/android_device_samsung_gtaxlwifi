		<ctl name="AudioMixer CH3 Rec En" value="On" />
		<ctl name="AudioMixer CH1 DOUT Select" value="RMIX_OUT" />
		<ctl name="AudioMixer CH3 DOUT Select" value="DMIX_OUT" />
	</path>

